<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="388.0" prefWidth="659.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
   <center>
      <TableView fx:id="TableV" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #333; -fx-border-color: #000;" BorderPane.alignment="CENTER" />
   </center>
   <left>
      <VBox prefHeight="388.0" prefWidth="163.0" style="-fx-background-color: #333;" BorderPane.alignment="CENTER">
         <children>
            <Label text="Наименование продукта" textFill="WHITE" />
            <TextField fx:id="Text1" />
            <Label text="Цена" textFill="WHITE" />
            <TextField fx:id="Text2" />
            <Pane prefHeight="25.0" prefWidth="171.0" />
            <Button fx:id="AddBut" mnemonicParsing="false" onAction="#AddBut_click" prefHeight="26.0" prefWidth="160.0" style="-fx-background-color: #000;" text="Добавить" textFill="WHITE" />
            <Pane prefHeight="10.0" prefWidth="171.0" />
            <Button fx:id="UpBut" mnemonicParsing="false" onAction="#UpBut_click" prefHeight="26.0" prefWidth="160.0" style="-fx-background-color: #000;" text="Изменить" textFill="WHITE" />
            <Pane prefHeight="10.0" prefWidth="171.0" />
            <Button fx:id="DelBut" mnemonicParsing="false" onAction="#DelBut_click" prefWidth="160.0" style="-fx-background-color: #000;" text="Удалить" textFill="WHITE" />
            <Pane prefHeight="10.0" prefWidth="171.0" />
            <Button fx:id="RefBut" mnemonicParsing="false" onAction="#RefBut_click" prefWidth="160.0" style="-fx-background-color: #000;" text="Обновить" textFill="WHITE" />
         </children>
      </VBox>
   </left>
</BorderPane>
